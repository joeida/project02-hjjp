<div class="row">
    <div class="col col-sm-4 col-sm-offset-4">
        <div id="signup" class="signup-form">
            <div class="panel panel-default">
                <div class="panel-body">
                    <form action="u/create" method="POST">
                        <div class="form-group">
                            <label for="address">Address</label>
                            <input type="text" class="form-control" id="addressInput" name="address" required>
                        </div>
                        <div class="form-group">
                            <label for="city">City</label>
                            <input type="text" class="form-control" id="cityInput" name="city" required>
                        </div>
                        <div class="form-group">
                            <label for="state">State</label>
                            <input type="text" class="form-control" id="stateInput" name="state" required>
                        </div>
                        <div class="signup-submit">
                            <button id="getSafezoneBtn" class="btn pull-right" type="submit">Look Up</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="modalCollection">
</div>

<div class="row">
    <div class="col col-sm-4 col-sm-offset-4">
        <div id="signup" class="signup-form">
            <div class="panel panel-default">
                <div id="mapLink" class="panel-body">
                </div>
            </div>
        </div>
    </div>
</div>
